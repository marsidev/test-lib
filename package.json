	"description": "This a simple package for testing create-typescript-app",
	"repository": {
		"type": "git",
		"url": "https://github.com/marsidev/test-lib"
	},
	"license": "MIT",
	"author": {
		"name": "Luis Marsiglia",
		"email": "marsiglia.business@gmail.com"
	},
	"type": "module",
