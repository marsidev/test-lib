		"@vitest/coverage-v8": "2.1.0",
		"@vitest/eslint-plugin": "1.3.3",
		"console-fail-test": "0.5.0",
		"eslint": "9.32.0",
		"eslint-plugin-n": "17.16.1",
		"husky": "9.1.4",
		"knip": "5.44.0",
		"lint-staged": "16.1.0",
		"prettier": "3.6.0",
		"prettier-plugin-curly": "0.3.0",
		"prettier-plugin-packagejson": "2.5.1",
		"prettier-plugin-sh": "0.18.0",
		"release-it": "19.0.1",
		"tsup": "8.5.0",
